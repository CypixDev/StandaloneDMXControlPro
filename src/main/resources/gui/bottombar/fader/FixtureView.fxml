<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="vbox" alignment="TOP_CENTER" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="ivFixtureImage" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="lFixtureName" />
   </children>
   <stylesheets>
      <URL value="@../../MainStyle.css" />
      <URL value="@fixturestyle.css" />
   </stylesheets>
</fx:root>
