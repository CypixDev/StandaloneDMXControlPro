<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" fx:id="vbox" alignment="TOP_CENTER" type="javafx.scene.layout.VBox"
         xmlns="http://javafx.com/javafx/19">
    <children>
        <ImageView fx:id="ivFixtureImage" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"/>
        <Label fx:id="lFixtureName"/>
    </children>
    <stylesheets>
        <URL value="@../../MainStyle.css"/>
        <URL value="@fixturestyle.css"/>
    </stylesheets>
</fx:root>
