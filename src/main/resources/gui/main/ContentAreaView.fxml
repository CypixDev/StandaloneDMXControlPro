<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<SplitPane xmlns:fx="http://javafx.com/fxml/1" fx:id="splitPane" dividerPositions="0.5" orientation="VERTICAL"
           style="-fx-background-color: -fx-color;" stylesheets="@../MainStyle.css" xmlns="http://javafx.com/javafx/19"
           fx:controller="de.standaloendmx.standalonedmxcontrolpro.gui.main.ContentAreaViewController">
    <items>
        <AnchorPane fx:id="content" style="-fx-background-color: -fx-color;">
            <children>
                <HBox fx:id="contentBox" prefHeight="100.0" prefWidth="200.0" spacing="10.0"
                      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                      AnchorPane.topAnchor="0.0"/>
            </children>
        </AnchorPane>
        <HBox prefHeight="508.0" prefWidth="1260.0" style="-fx-background-color: -fx-color;"
              stylesheets="@../MainStyle.css">
            <children>
                <VBox spacing="5.0" style="-fx-background-color: -fx-color;" stylesheets="@../MainStyle.css">
                    <children>
                        <Button mnemonicParsing="false">
                            <graphic>
                                <FontIcon iconLiteral="anto-appstore-add" iconSize="64"/>
                            </graphic>
                        </Button>
                        <Button mnemonicParsing="false">
                            <graphic>
                                <FontIcon iconLiteral="anto-sliders" iconSize="64"/>
                            </graphic>
                        </Button>
                        <Button mnemonicParsing="false">
                            <graphic>
                                <FontIcon iconLiteral="anto-play-square" iconSize="64"/>
                            </graphic>
                        </Button>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets left="5.0"/>
                    </padding>
                </VBox>
            </children>
        </HBox>
    </items>
</SplitPane>
