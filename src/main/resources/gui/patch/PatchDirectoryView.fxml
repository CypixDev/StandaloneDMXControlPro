<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<VBox fx:id="mainBox" prefWidth="266.0" spacing="5.0" style="-fx-background-color: -fx-color;" styleClass="main-vbox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.standaloendmx.standalonedmxcontrolpro.gui.patch.PatchDirectoryViewController">
   <children>
      <HBox alignment="CENTER_LEFT" style="-fx-background-color: -fx-color;" stylesheets="@style.css" VBox.vgrow="NEVER">
         <children>
            <Button mnemonicParsing="false">
               <graphic>
                  <FontIcon iconLiteral="anto-bulb" iconSize="18" />
               </graphic>
            </Button>
            <Label text="Fixture Library" />
            <Button mnemonicParsing="false">
               <graphic>
                  <FontIcon iconLiteral="anto-cloud-download" iconSize="18" />
               </graphic>
            </Button>
            <Button fx:id="btnFolder" mnemonicParsing="false">
               <graphic>
                  <FontIcon iconLiteral="anto-folder" iconSize="18" />
               </graphic>
            </Button>
         </children>
      </HBox>
      <TextField prefHeight="25.0" promptText="Search" VBox.vgrow="NEVER">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets left="5.0" right="5.0" />
         </VBox.margin>
      </TextField>
      <TreeView fx:id="directory" prefWidth="264.0" style="-fx-background-color: -fx-color;" VBox.vgrow="ALWAYS" />
   </children>
   <stylesheets>
      <URL value="@../MainStyle.css" />
      <URL value="@style.css" />
   </stylesheets>
</VBox>
